datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model tb_user {
  id       String   @id @db.Uuid
  name     String   @db.VarChar(50)
  birthage DateTime @db.Date
  email    String   @unique @db.VarChar(50)
  password String   @db.Text
}
